<description>
  <types>
	<schema>
	  <element name="CalendarRecord">
		<complexType>
		  <sequence>
			<element name="Date" type="xsd:datetime"/>
			<element name="TimeOffType" type="xsd:string"/>
			<element name="Status" type="xsd:string"/>
			<element name="Hours" type="xsd:double"/>
			<element name="Notes" type="xsd:string"/>
			<element name="HolidayName" type="xsd:string"/>
		  </sequence>
		</complexType>
	  </element>
	</schema>
  </types>

  <message name="TimeOffInfoRequest">
	<part name="FromSystemId" type="xsd:string"/>
	<part name="UserId" type="xsd:string"/>
	<part name="SystemId" type="xsd:int"/>
	<part name="RangeStart" type="xsd:datetime"/>
	<part name="RangeEnd" type="xsd:datetime"/>
  </message>

  <message name="TimeOffInfoResponse">
	<part name="ServiceDate" type="xsd:datetime"/>
	<part name="PTOEstimated" type="xsd:double"/>
	<part name="PTOUsed" type="xsd:double"/>
	<part name="PTORequested" type="xsd:double"/>
	<part name="PTORemaining" type="xsd:double"/>
	<part name="FloatingHolidaysUsed" type="xsd:int"/>
	<part name="FloatingHolidaysReqeuested" type="xsd:int"/>
	<part name="FloatingHolidaysRemaining" type="xsd:int"/>

	<element maxOccurs="unbounded" name="CalendarRecords" type="xsd1:CalendarRecord"/>
  </message>

  <portType name="TimeOff">
	<operation name="GetTimeOffInfo">
	  <input message="TimeOffInfoRequest"/>
	  <output message="TimeOffInfoResponse"/>
	</operation>
  </portType>
</description>