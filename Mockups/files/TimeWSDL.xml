			<element name="DistScale" type="xsd:string"/>
			<element name="DistScaleAccom" type="xsd:string"/>
			<element name="DistToSite" type="xsd:double"/>
			<element name="DistToSiteAccom" type="xsd:double"/>
			<element name="FieldBreakDays" type="xsd:int"/>
			<element name="FLA" type="xsd:double"/>
			<element name="IHRISID" type="xsd:int"/>
			<element name="JobComplete" type="xsd:bool"/>
			<element name="JobLocation" type="xsd:string"/>
			<element name="Location" type="xsd:string"/>
			<element name="Night" type="xsd:bool"/>
			<element name="OnSiteSince" type="xsd:date"/>
			<element name="Platform" type="xsd:string"/>
			<element name="Radius" type="xsd:string"/>
			<element name="Reason" type="xsd:string"/>
			<element name="ServiceOrder" type="xsd:string"/>
			<element name="Shift" type="xsd:string"/>
			<element name="SiteComplete" type="xsd:bool"/>
			<element name="SiteCountry" type="xsd:string"/>
			<element name="SiteManager" type="xsd:string"/>
			<element name="ToolingUsed" type="xsd:string"/>
			<element name="UnitNumber" type="xsd:string"/>
			<element name="WorkLocation" type="xsd:string"/>

			<element maxOccurs="unbounded" name="TransactionDetails" type="xsd1:TransactionDetail"/>
