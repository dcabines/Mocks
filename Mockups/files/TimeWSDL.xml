			<element name="Overnight" type="xsd:bool"/>
			<element name="OvertimeinMoney" type="xsd:bool"/>
			<element name="OvertimeinTime" type="xsd:bool"/>
			<element name="PerDiem" type="xsd:double"/>
			<element name="PercentComplete" type="xsd:double"/>
			<element name="Phase" type="xsd:string"/>
			<element name="PremiumAllowed" type="xsd:bool"/>
