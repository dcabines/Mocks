			<element name="WorkOrder" type="xsd:string"/>
		  </sequence>
		</complexType>
	  </element>
	</schema>
  </types>

  <message name="TransactionHeaders">
	<part name="TransactionHeaders" type="xsd1:TransactionHeader"  maxOccurs="unbounded" />
  </message>

  <portType name="Transactions">
	<operation name="UploadTimeTransactions">
	  <input name="TransactionHeaders" message="TransactionHeaders"/>
	</operation>
  </portType>
</description>